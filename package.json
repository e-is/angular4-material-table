{
  "name": "@e-is/ngx-material-table",
  "description": "Angular 11 table based on @angular/cdk table structure, to allow row insertion, edition, validation and deletion.",
  "version": "0.12.1",
  "license": "MIT",
  "author": "Ignacio Rossi <irossimoline>",
  "keywords": [
    "angular",
    "angular 11",
    "table",
    "inline validation",
    "material design",
    "angular material table",
    "ngx material table",
    "ngx table"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/e-is/ngx-material-table.git"
  },
  "homepage": "https://github.com/e-is/ngx-material-table",
  "bugs": {
    "url": "https://github.com/e-is/ngx-material-table/issues"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4201",
    "build": "ng build --prod",
    "lint": "ng lint",
    "packagr": "ng-packagr -p ng-package.json",
    "publish": "npm run packagr && cd dist && npm publish",
    "publish-dev": "npm run packagr && cd dist && npm publish --userconfig=.local/.npmrc"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "bundles/ngx-material-table.umd.js",
  "module": "index.js",
  "typings": "index.d.ts",
  "private": false,
  "dependencies": {
    "clone": "^2.1.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.19",
    "@angular/animations": "~11.2.14",
    "@angular/cdk": "~11.2.13",
    "@angular/cli": "~11.2.19",
    "@angular/common": "~11.2.14",
    "@angular/compiler": "~11.2.14",
    "@angular/compiler-cli": "~11.2.14",
    "@angular/core": "~11.2.14",
    "@angular/forms": "~11.2.14",
    "@angular/router": "~11.2.14",
    "@angular/language-service": "~11.2.14",
    "@angular/material": "~11.2.13",
    "@angular/platform-browser": "~11.2.14",
    "@angular/platform-browser-dynamic": "~11.2.14",
    "@rollup/plugin-commonjs": "~17.0.0",
    "@rollup/plugin-node-resolve": "~11.2.1",
    "@types/jasmine": "3.6.2",
    "@types/node": "~12.19.8",
    "codelyzer": "~6.0.1",
    "core-js": "^3.8.0",
    "material-design-icons": "^3.0.1",
    "ng-packagr": "~11.2.4",
    "sass": "^1.29.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.34.0",
    "rxjs": "6.6.3",
    "ts-node": "~9.0.0",
    "tslib": "~2.0.3",
    "tslint": "~6.1.3",
    "typescript": "~4.0.5",
    "uglify-es": "^3.3.10",
    "zone.js": "^0.11.3"
  }
}
